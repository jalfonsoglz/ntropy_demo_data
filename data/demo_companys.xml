    <!--    Creación de Empresas -->
    <!--    1. Services -->
    <record id="base.main_company" model="res.company">
        <field name="sequence">0</field>
        <field name="name">ERP Solutions</field>
        <field name="partner_id" ref="base.main_partner"/>
        <!--    Asociación de Certificados  -->
        <field name="l10n_mx_edi_pac">solfact</field>
        <field name="l10n_mx_edi_pac_test_env">true</field>
        <field name="l10n_mx_edi_fiscal_regime">601</field>
        <!--    Configuraciones Generales de Compañia  -->
        <field name="font">Open_Sans</field>
        <field name="paperformat_id">1</field>
        <field name="external_report_layout_id">200</field>
        <field name="font">Tajawal</field>
        <field name="primary_color">#0d9d9a</field>
        <field name="secondary_color">#111219</field>
        <field name="layout_background">Blank</field>
        <field name="account_fiscal_country_id">156</field>
        <field name="currency_provider">banxico</field>
        <field name="currency_interval_unit">daily</field>
        <field name="currency_next_execution_date">2024-01-01</field>
        <field name="l10n_mx_edi_num_exporter">123456789</field>
<!--        <field name="tax_cash_basis_journal_id" ref="account.1_cuenta801_01_99"/>-->
    </record>
    <!--    Paquete Localización Fiscal  -->
    <function model="account.chart.template" name="try_loading">
        <value eval="[]"/>
        <value>mx</value>
        <value model="res.company" eval="ref('base.main_company')"/>
    </function>

