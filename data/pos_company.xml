            <field name="vat">GOSJ870817T63</field>
            <field name="street">Campobasso Norte 32006</field>
            <field name="street2">Colonia Juárez</field>
            <field name="street_name">Campobasso Norte 32006</field>
            <field name="street_number">10</field>
            <field name="street_number2">15</field>
            <field name="phone">5511223344</field>
            <field name="mobile">5511223344</field>
            <field name="email">admin@example.com</field>
            <field name="website">www.mxexample.com</field>
            <field name="lang">es_MX</field>
            <field name="city">Jesús Maria</field>
