            <field name="default_purchase_method">purchase</field>
            <field name="use_invoice_terms">1</field>
            <field name="digest_emails">false</field>
            <field name="module_base_geolocalize">true</field>
            <field name="map_box_token">
                pk.eyJ1IjoiYWxmb25zb2dsenNseiIsImEiOiJja21kdjZqNG4wYWNxMndwcTlwdXR2YTl0In0.qnzfm04m8aYwxFghzezI7w
            </field>
            <field name="module_l10n_mx_edi">true</field>
            <field name="module_account_budget">true</field>
            <field name="module_partner_autocomplete">false</field>
            <field name="module_product_margin">true</field>
            <field name="module_product_expiry">true</field>
