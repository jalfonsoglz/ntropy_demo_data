        </record>

        <!--        2. Creacion de Compañia-->

        <record id="second_company" model="res.company">
            <field name="name">POS-Store</field>
            <field name="partner_id" ref="ntropy_demo_data_generator.second_pos_partner"/>
            <field name="chart_template">mx</field>
            <field name="logo" type="base64" file="ntropy_demo_data_generator/static/icons/pos_company.png"/>

            <!--Configuraciones default de la Compañia-->

            <field name="paperformat_id">1</field>
            <field name="external_report_layout_id">200</field>
            <field name="font">Montserrat</field>
            <field name="primary_color">#AF4F4F</field>
            <field name="secondary_color">#AFA5A5</field>
            <field name="layout_background">Blank</field>
            <field name="account_fiscal_country_id">156</field>
            <field name="currency_provider">banxico</field>
            <field name="currency_interval_unit">daily</field>
            <field name="currency_next_execution_date">2024-01-01</field>
            <field name="l10n_mx_edi_num_exporter">123456789</field>
        </record>

        <function model="res.company" name="_onchange_country_id">
            <value eval="[ref('second_company')]"/>
        </function>

